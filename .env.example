# ─── Helius API (required) ────────────────────────────────────
# Sign up at https://dashboard.helius.dev
HELIUS_API_KEY=your_helius_api_key_here

# ─── Pipeline Config ─────────────────────────────────────────
# Max concurrent RPC requests
MAX_CONCURRENCY=10

# 0 = scan all wallets, >0 = sample N wallets (useful for free tier)
HOLDINGS_SAMPLE_SIZE=0

# ─── Optional ────────────────────────────────────────────────
# Dashboard JSON path (relative to pipeline/)
DASHBOARD_OUTPUT=../data/dashboard.json

# CoinGecko API key (optional, free tier works without)
COINGECKO_API_KEY=
